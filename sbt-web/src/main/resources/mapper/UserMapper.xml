<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.4//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kubrick.sbt.web.dao.UserDao">
	<resultMap type="com.kubrick.sbt.web.entity.User" id="user">
		<id property="id" column="id"/>
		<result property="username" column="username"/>
		<result property="password" column="password"/>
		<result property="createBy" column="create_by"/>
	</resultMap>

	<select id="getUserByUsername" resultMap="user">
		select * from t_user where username=#{username}
	</select>

	<insert id="insertUser" parameterType="com.kubrick.sbt.web.entity.User" keyProperty="id" useGeneratedKeys="true">
		insert into t_user (username,password,create_by) values(#{username},#{password},#{createBy})
	</insert>

	<select id="list" resultType="com.kubrick.sbt.web.entity.User">

		select * from t_user

	</select>
</mapper>

